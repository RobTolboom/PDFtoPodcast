{
  "$id": "test_study.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Test fixture for study schema that references common schema",
  "type": "object",
  "properties": {
    "schema_version": {
      "type": "string",
      "const": "v1.0-test"
    },
    "metadata": {
      "$ref": "test_common.schema.json#/$defs/Metadata"
    },
    "results": {
      "type": "object",
      "properties": {
        "outcome": {"type": "string"},
        "p_value": {"type": "number"}
      }
    }
  },
  "required": ["schema_version", "metadata"]
}
